# Electric Flying Swan
#
# Set to 1 to have vehicle return home when not in use
AUTORETURN=1
#
# Volume for effects from 0 to 10
VOLUME=10
#
# Default language
LANG=en-GB
#
# CAR  or BOAT  or  PLANE
V_TYPE=PLANE
#
VEHICLE_BUOYANCY=1.0
#
# Prim numbers for any rotating wheels
FL=13
FR=14
BL=15
BR=16
#
# Optional rotation in case the wheels do not spin across the X axis
WHEEL_ROTATION=<0.0, 0.0, PI/2>
#
# Prim number for particle effects
FX_PRIM=1
#
SIT_TARGET=<-0.1, 0.1, 0.1>
SIT_ROT=<0.0, 0.0, 0.0>
#
FWD_POWER=12.0
REV_POWER=-5.0
#
# Less is sharper range 0 to 10
TURN_RATION=2.0
